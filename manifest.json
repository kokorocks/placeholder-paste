{
    "manifest_version": 3,
    "name": "Focus Placeholder Checker",
    "version": "1.1",
    "description": "Alerts the placeholder of input fields when focused.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end",
        "all_frames": true
      }
    ]
  }
  